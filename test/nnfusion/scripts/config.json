{
  "env": {
    "PYTHONDONTWRITEBYTECODE" : {
      "set" : 1
    },
    "HSA_USERPTR_FOR_PAGED_MEM": {
      "set": 0
    },
    "LD_LIBRARY_PATH": {
      "append": "/usr/local/lib"
    },
    "HIP_VISIBLE_DEVICES" : {
      "set": 1
    }
  },
  "device_capability": ["CUDA", "ROCM"],
  "enabled_tags": {
    "CUDA" : ["correctness", "correctness_cuda_only"],
    "ROCM" : ["correctness", "correctness_rocm_only"]
  }
}
